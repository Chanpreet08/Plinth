        <activity android:name="lnmiit.madclub.plinth.Activity.WebomasterActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="lnmiit.madclub.plinth.Activity.AugmentedActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="lnmiit.madclub.plinth.Activity.CompleteLoginActivity"
            android:screenOrientation="portrait"/>
