<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lnmiit.plinth">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".Activity.MyApplication"

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />

        <activity
            android:screenOrientation="portrait"
            android:name=".Activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name=".Activity.SplashActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar.FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Activity.AstronomyActivity" />
        <activity
            android:screenOrientation="portrait"
            android:name=".Activity.LoginActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar.FullScreen" />
        <activity
            android:name="com.facebook.CustomTabActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="@string/fb_login_protocol_scheme" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.CodingActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.RoboticsActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.QuizzingActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.LiteratureActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.ManagementActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.ArmageddonActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.AstroHuntActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.StarTrekActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.FixthebugActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.IupcActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.DistarctionIupcActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.LfrActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.QuadcopterActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.TransporterActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.RoboraceActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.RobowarActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.RoboSoccerActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.QuestActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.WrangleActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.RostrumActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.SifActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.VehicleActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.WebomasterActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.AugmentedActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.CompleteLoginActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.PhotoGraphyWorkshopActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.CompleteRegisterActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Activity.PrinterActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>